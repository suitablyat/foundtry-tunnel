{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "FoundryTunnel",
  "version": "2.0.0",
  "identifier": "com.lappenbande.foundrytunnel",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run vite:dev",
    "beforeBuildCommand": "npm run vite:build"
  },
  "app": {
    "windows": [
      {
        "title": "Foundry Tunnel",
        "width": 600,
        "height": 690,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
